{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## 📝 Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## 🔧 Maintenance",
      "labels": ["maintenance", "refactor"]
    }
  ],
  "template": "${{CHANGELOG}}\n\n## Other Changes\n${{UNCATEGORIZED}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "^feat.*",
      "target": "feature"
    },
    {
      "pattern": "^fix.*",
      "target": "bug"
    },
    {
      "pattern": "^docs.*",
      "target": "documentation"
    },
    {
      "pattern": "^refactor.*",
      "target": "maintenance"
    }
  ]
}
